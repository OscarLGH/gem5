format StoreIndexOp {
    135: stvebx({{
        Msr msr_v = MSR;
        uint8_t eb = bits(EA, 3, 0);
        __uint128_t val_128 = Vs_uq[0];
        uint8_t *val = (uint8_t *)&val_128;
        if (msr_v.le) {
            Mem_ub = val[eb];
        } else {
            Mem_ub = val[15 - eb];
        }
    }});
}

